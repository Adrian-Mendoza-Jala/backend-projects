<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <SolidColorBrush x:Key="HeaderBackgroundBrush" Color="#F05A28"/>
  <SolidColorBrush x:Key="HeaderForegroundBrush" Color="White"/>
  <ResourceDictionary.MergedDictionaries>
    <!-- Load WinUI resources -->
    <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />

  </ResourceDictionary.MergedDictionaries>
  <ResourceDictionary.ThemeDictionaries>
    <ResourceDictionary x:Key="Dark">
      <SolidColorBrush x:Key="NavigatioBackgroundBrush" Color="#333" />
    </ResourceDictionary>
    <ResourceDictionary x:Key="Light">
      <SolidColorBrush x:Key="NavigatioBackgroundBrush" Color="#BBB" />
    </ResourceDictionary>
  </ResourceDictionary.ThemeDictionaries>
  <!-- Add resources here -->

</ResourceDictionary>
